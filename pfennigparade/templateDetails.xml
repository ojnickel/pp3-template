<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/1.6/template-install.dtd">
<extension version="3.0" type="template" client="site">
	<name>pfennigparade</name>
	<creationDate>06.05.2015</creationDate>
	<author>OpenWebServices based on beez by Angie Radtke/Robert Deutz</author>
	<authorEmail>openweb@pfennigparade.de</authorEmail>
	<authorUrl>http://www.pfennigparade.de</authorUrl>
	<copyright></copyright>
	<license>GNU/GPL</license>
	<version>3.0.0</version>
	<description>Accessible template for Joomla 3.x</description>

	<files>
		<folder>code</folder>
    <folder>css</folder>
		<folder>images</folder>
		<folder>html</folder>
    <folder>javascript</folder>
		<filename>index.html</filename>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_thumbnail.png</filename>
		<filename>favicon.ico</filename>
		<filename>component.php</filename>
		<filename>template_preview.png</filename>
		<filename>error.php</filename>
	</files>

	<languages>
		<language tag="en-GB">en-GB.tpl_pfennigparade.ini</language>
	</languages>
	<administration>
		<languages folder="admin">
			<language tag="en-GB">en-GB.tpl_beez.ini</language>
		</languages>
	</administration>

	<positions>
		<position>left</position>
		<position>right</position>
		<position>top</position>
		<position>breadcrumb</position>
		<position>banners</position>
		<position>user1</position>
		<position>user2</position>
		<position>user3</position>
		<position>user4</position>
		<position>debug</position>
		<position>syndicate</position>
    <position>breadcrumb_dp</position>        
		<position>startseite</position>
    <position>intro_dp</position>
		<position>intro2_dp</position>
		<position>contentnavi</position>
		<position>contentnavi2</position>
		<position>contentnavi3</position>
		<position>contentnavi4</position>
		<position>infoboxen</position> 
		<position>infoboxeno2</position>
    <position>unseen</position>
	</positions>

	<config>
	</config>

</extension>

